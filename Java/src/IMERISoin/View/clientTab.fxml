<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IMERISoin.Controller.PatientController">
    <SplitPane dividerPositions="0.49150622876557193">
        <AnchorPane minWidth="300.0">
            <Button fx:id="clientNewButton" layoutX="196.0" layoutY="88.0" mnemonicParsing="false" onAction="#newPatientAction" text="new Patient" />
            <Label layoutX="21.0" layoutY="20.0" text="n° Sécurité Sociale" />
            <TextField fx:id="clientNameField" layoutX="18.0" layoutY="37.0" />
            <Label layoutX="18.0" layoutY="130.0" text="Status" />
            <ChoiceBox fx:id="clientStatusBox" layoutX="18.0" layoutY="147.0" prefHeight="25.0" prefWidth="149.0" />
         <Button fx:id="setPatientStatusAction" layoutX="196.0" layoutY="147.0" mnemonicParsing="false" onAction="#setPatientStatusAction" prefWidth="78.0" text="Set Status" />
         <ChoiceBox fx:id="roomStatusBox" layoutX="18.0" layoutY="88.0" prefHeight="25.0" prefWidth="149.0" />
        </AnchorPane>
        <AnchorPane>
            <TableView fx:id="patientTable" layoutX="2.0" prefHeight="591.0" prefWidth="446.0">
                <columns>
                    <TableColumn fx:id="nSSColumn" prefWidth="75.0" text="n° Sécurité Sociale" />
                    <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>
        </AnchorPane>
    </SplitPane>
</AnchorPane>
